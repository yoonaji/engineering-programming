def convert_date_format(n):
  #re 모듈 가져오기
  import re
  #숫자 여러개-숫자여러개-숫자여러개 그루핑 형식으로 컴파일 지정해주기
  a = re.compile('(\d+)-(\d+)-(\d+)')
  #search 매서드로 문자열에서 패턴과 일치하는 부분 찾기
  match = a.search(n)
  #만약 매치되면
  if match:
    #두번째그룹/세번째그룹/첫번째그룹 프린트해주기
    print(match.group(2),'/',match.group(3),'/',match.group(1))
#함수 실행
convert_date_format('2023-06-06')
